{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "game-of-life-tauri",
	"version": "0.1.0",
	"identifier": "com.game-of-life-tauri.app",
	"build": {
		"beforeDevCommand": "yarn dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "yarn build",
		"frontendDist": "../dist"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"title": "game-of-life-tauri",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": []
	}
}
